<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="300.0" prefWidth="500.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="ViewProfile.ProfileController">
   <children>
      <Label layoutX="27.0" layoutY="42.0" prefHeight="32.0" prefWidth="120.0" text="Libellé du Profile" />
      <TextField fx:id="libelle_txt" layoutX="14.0" layoutY="93.0" prefHeight="32.0" prefWidth="149.0" />
      <TableView fx:id="profile_table" layoutX="187.0" layoutY="40.0" onMouseClicked="#tableClick" prefHeight="181.0" prefWidth="285.0">
        <columns>
          <TableColumn fx:id="id_column" prefWidth="137.0" text="Identifiant" />
          <TableColumn fx:id="libelle_column" prefWidth="147.0" text="Libellé" />
        </columns>
      </TableView>
      <Button fx:id="valider_btn" layoutX="44.0" layoutY="151.0" mnemonicParsing="false" onAction="#valider" prefHeight="25.0" prefWidth="72.0" text="Valider" />
      <Button fx:id="actualiser_btn" layoutX="124.0" layoutY="250.0" mnemonicParsing="false" onAction="#actualiser" text="Actualiser" />
      <Button fx:id="modifier_btn" layoutX="235.0" layoutY="251.0" mnemonicParsing="false" onAction="#modifier" text="Modifier" />
      <Button fx:id="supprimer_btn" layoutX="347.0" layoutY="251.0" mnemonicParsing="false" onAction="#supprimer" text="Supprimer" />
